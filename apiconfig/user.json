			},
			{
				"name": "desc",
				"type": "string",
				"desc": "描述信息",
				"default": ""
			}
		]
	},

	"APIUserLogOut": {
		"name": "退出登录",
		"handler": "AddUser",
		"paras": [
			{
				"name": "sessionUuid",
				"type": "string",
				"desc": "账号UUID",
				"default": "NotNull"
			}
		]
	}
