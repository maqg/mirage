{
   	"APIAddUser": {
		"name": "添加账号",
		"handler": "AddUser",
		"paras": [
			{
				"name": "account",
				"type": "string",
				"desc": "账号名称",
				"default": "NotNull"
			},
			{
				"name": "password",
				"type": "string",
				"desc": "密码（明文）",
				"default": "NotNull"
			},
			{
				"name": "email",
				"type": "string",
				"desc": "账号名称",
				"default": ""
			},
			{
				"name": "phoneNumber",
				"type": "string",
				"desc": "电话号码",
				"default": ""
			},
			{
				"name": "type",
				"type": "int",
				"desc": "账号类型",
				"default": 3
			},
			{
				"name": "desc",
				"type": "string",
				"desc": "描述信息",
				"default": ""
			}
		]
	},

	"APILoginByUser": {
		"name": "根据账号登录",
		"handler": "AddUser",
		"paras": [
			{
				"name": "account",
				"type": "string",
				"desc": "账号名称",
				"default": "NotNull"
			},
			{
				"name": "password",
				"type": "string",
				"desc": "密码（明文）",
				"default": "NotNull"
			}
		]
	},

	"APIShowUser": {
		"name": "获取单个账号信息",
		"handler": "AddUser",
		"paras": [
			{
				"name": "id",
				"type": "string",
				"desc": "账号的UUID",
				"default": "NotNull"
			}
		]
	},

	"APIShowAllUser": {
		"name": "获取所有账号信息",
		"handler": "AddUser",
		"paras": [
			{
				"name": "start",
				"type": "int",
				"desc": "开始位置",
				"default": 0
			},
			{
				"name": "limit",
				"type": "int",
				"desc": "获取条目",
				"default": 15
			}
		]
	},

	"APIShowUserList": {
		"name": "获取账号列表",
		"handler": "AddUser",
		"paras": []
	},

	"APIDeleteUser": {
		"name": "删除单个账号信息",
		"handler": "AddUser",
		"paras": [
			{
				"name": "id",
				"type": "string",
				"desc": "账号的UUID",
				"default": "NotNull"
			}
		]
	},

	"APIResetUserPassword": {
		"name": "重置账号密码",
		"handler": "AddUser",
		"paras": [
			{
				"name": "id",
				"type": "listint",
				"desc": "账号的UUID",
				"default": "NotNull"
			},
			{
				"name": "password",
				"type": "string",
				"desc": "新密码",
				"default": "NotNull"
			}
		]
	},

